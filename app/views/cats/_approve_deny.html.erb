<% if request.pending? %>
<td>
  <form action="<%= cat_rental_request_url(request.id)%>/approve" method="post">
    <input type="submit" value="Approve">
  </form>

  <form action="<%= cat_rental_request_url(request.id)%>/deny" method="post">
    <input type="submit" value="Deny">
  </form>
</td>
<% end %>
